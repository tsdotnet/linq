{
  "name": "@tsdotnet/linq",
  "version": "1.5.0",
  "description": "A familiar set of functions that operate on JavaScript iterables (ES2015+) in a similar way to .NET's LINQ does with enumerables.",
  "author": "electricessence",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "types": "./dist/types/index.d.ts",
  "keywords": [
    "linq",
    "iterator",
    "iterators",
    "iterable",
    "iterables",
    "enumerable",
    "es2015",
    "filter",
    "map",
    "select",
    "reduce",
    "aggregate",
    "where",
    "order by",
    "order-by",
    "orderBy",
    "group by",
    "group-by",
    "groupBy",
    "filters",
    "transforms",
    "resolutions",
    "resolve"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build:types": "cpy \"src/**/*.d.ts\" dist/types --parents && tsc -p tsconfig.types.json",
    "build:esm": "rollup -c rollup.config.js",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build": "pnpm clean && pnpm build:types && pnpm build:esm && pnpm build:cjs && pnpm typecheck:tests",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "typecheck:tests": "tsc -p tests/tsconfig.json --noEmit",
    "lint": "eslint src/**/*.ts",
    "docs": "rimraf docs && typedoc --options typedoc.json --readme none",
    "bump": "pnpm run precommit && pnpm version patch",
    "precommit": "pnpm build && pnpm test && pnpm lint && git status -s",
    "preversion": "pnpm lint && pnpm test",
    "postversion": "git push && git push --tags && pnpm run docs && git add -A && git commit -m \"Updated docs.\" docs && git push",
    "prepublishOnly": "pnpm build && pnpm test && pnpm lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsdotnet/linq.git"
  },
  "bugs": {
    "url": "https://github.com/tsdotnet/linq/issues"
  },
  "homepage": "https://github.com/tsdotnet/linq#readme",
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/node": "^24.3.0",
    "@vitest/coverage-v8": "^3.2.4",
    "cpy-cli": "^6.0.0",
    "eslint": "^9.34.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.48.0",
    "typedoc": "^0.28.10",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@tsdotnet/common-interfaces": "^1.0.5",
    "@tsdotnet/compare": "^1.7.0",
    "@tsdotnet/exceptions": "^1.2.4",
    "@tsdotnet/integer": "^1.2.3",
    "@tsdotnet/linked-node-list": "^1.4.3",
    "@tsdotnet/memoize": "^1.2.2",
    "@tsdotnet/queue": "^1.3.1",
    "tslib": "^2.8.1"
  },
  "pnpm": {
    "overrides": {
      "hosted-git-info@<2.8.9": ">=2.8.9",
      "y18n@=4.0.0": ">=4.0.1",
      "lodash@<4.17.20": ">=4.17.20",
      "shell-quote@<=1.7.2": ">=1.7.3",
      "lodash@<4.17.21": ">=4.17.21",
      "json5@<1.0.2": ">=1.0.2",
      "json5@>=2.0.0 <2.2.2": ">=2.2.2",
      "minimatch@<3.0.5": ">=3.0.5",
      "semver@<5.7.2": ">=5.7.2",
      "semver@>=6.0.0 <6.3.1": ">=6.3.1",
      "semver@>=7.0.0 <7.5.2": ">=7.5.2",
      "get-func-name@<2.0.1": ">=2.0.1",
      "@babel/traverse@<7.23.2": ">=7.23.2",
      "braces@<3.0.3": ">=3.0.3"
    }
  }
}
