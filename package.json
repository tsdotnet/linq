{
  "name": "@tsdotnet/linq",
  "version": "1.2.0",
  "description": "A familiar set of functions that operate on JavaScript iterables (ES2015+) in a similar way to .NET's LINQ does with enumerables.",
  "author": "electricessence",
  "license": "MIT",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "module": "dist-esm/index.js",
  "keywords": [
    "linq",
    "iterator",
    "iterators",
    "iterable",
    "iterables",
    "enumerable",
    "es2015",
    "filter",
    "map",
    "select",
    "reduce",
    "aggregate",
    "where",
    "order by",
    "order-by",
    "orderBy",
    "group by",
    "group-by",
    "groupBy",
    "filters",
    "transforms",
    "resolutions",
    "resolve"
  ],
  "scripts": {
    "build:dist": "rimraf dist/* && tsc && copyfiles -u 1 src/**/*.d.ts dist",
    "build:dist-esm": "rimraf dist-esm/* && tsc -p tsconfig.esm.json",
    "build": "pnpm i && pnpm run build:dist && pnpm run build:dist-esm",
    "bump": "npm run-script precommit && npm version patch",
    "docs": "rimraf docs/* && typedoc --options typedoc.json --readme none",
    "lint": "eslint src/**/*.ts",
    "precommit": "pnpm i && run-p lint test && run-p build:* && npm run-script validate && git status -s",
    "prepublishOnly": "npm run-script build && run-p validate test",
    "preversion": "run-p lint test",
    "postversion": "git push && git push --tags && npm run-script docs && git commit -m \"Updated docs.\" docs && git push",
    "test": "mocha -r ts-node/register tests/**/*.ts",
    "validate": "node ./.build/validate-package.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsdotnet/linq.git"
  },
  "bugs": {
    "url": "https://github.com/tsdotnet/linq/issues"
  },
  "homepage": "https://github.com/tsdotnet/linq#readme",
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "chai": "^4.3.6",
    "copyfiles": "^2.4.1",
    "eslint": "^8.8.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "mocha": "^9.2.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typedoc": "^0.22.11",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@tsdotnet/common-interfaces": "^1.0.4",
    "@tsdotnet/compare": "^1.3.17",
    "@tsdotnet/exceptions": "^1.0.17",
    "@tsdotnet/integer": "^1.0.12",
    "@tsdotnet/linked-node-list": "^1.3.17",
    "@tsdotnet/memoize": "^1.0.9",
    "@tsdotnet/queue": "^1.2.5",
    "tslib": "^2.3.1"
  }
}
