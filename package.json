{
  "name": "@tsdotnet/linq",
  "version": "1.4.4",
  "description": "A familiar set of functions that operate on JavaScript iterables (ES2015+) in a similar way to .NET's LINQ does with enumerables.",
  "author": "electricessence",
  "license": "MIT",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "module": "dist-esm/index.js",
  "keywords": [
    "linq",
    "iterator",
    "iterators",
    "iterable",
    "iterables",
    "enumerable",
    "es2015",
    "filter",
    "map",
    "select",
    "reduce",
    "aggregate",
    "where",
    "order by",
    "order-by",
    "orderBy",
    "group by",
    "group-by",
    "groupBy",
    "filters",
    "transforms",
    "resolutions",
    "resolve"
  ],
  "scripts": {
    "build:dist": "rimraf dist && tsc && copyfiles -u 1 src/**/*.d.ts dist",
    "build:dist-esm": "rimraf dist-esm && tsc -p tsconfig.esm.json",
    "build": "pnpm i && pnpm run build:dist && pnpm run build:dist-esm",
    "bump": "npm run-script precommit && npm version patch",
    "docs": "rimraf docs && typedoc --options typedoc.json --readme none",
    "lint": "eslint src/**/*.ts",
    "precommit": "pnpm i && run-p lint test && run-p build:* && npm run-script validate && git status -s",
    "prepublishOnly": "npm run-script build && run-p validate test",
    "preversion": "run-p lint test",
    "postversion": "git push && git push --tags && npm run-script docs && git add -A && git commit -m \"Updated docs.\" docs && git push",
    "test": "mocha -r ts-node/register tests/**/*.ts",
    "validate": "node ./.build/validate-package.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsdotnet/linq.git"
  },
  "bugs": {
    "url": "https://github.com/tsdotnet/linq/issues"
  },
  "homepage": "https://github.com/tsdotnet/linq#readme",
  "devDependencies": {
    "@types/chai": "^5.2.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "chai": "^5.2.1",
    "copyfiles": "^2.4.1",
    "eslint": "^9.33.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^7.2.1",
    "mocha": "^11.7.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^17.1.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.10",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@tsdotnet/common-interfaces": "^1.0.4",
    "@tsdotnet/compare": "^1.3.22",
    "@tsdotnet/exceptions": "^1.0.19",
    "@tsdotnet/integer": "^1.0.15",
    "@tsdotnet/linked-node-list": "^1.3.20",
    "@tsdotnet/memoize": "^1.0.10",
    "@tsdotnet/queue": "^1.2.8",
    "tslib": "^2.4.1"
  },
  "pnpm": {
    "overrides": {
      "hosted-git-info@<2.8.9": ">=2.8.9",
      "y18n@=4.0.0": ">=4.0.1",
      "lodash@<4.17.20": ">=4.17.20",
      "shell-quote@<=1.7.2": ">=1.7.3",
      "lodash@<4.17.21": ">=4.17.21",
      "json5@<1.0.2": ">=1.0.2",
      "json5@>=2.0.0 <2.2.2": ">=2.2.2",
      "minimatch@<3.0.5": ">=3.0.5",
      "semver@<5.7.2": ">=5.7.2",
      "semver@>=6.0.0 <6.3.1": ">=6.3.1",
      "semver@>=7.0.0 <7.5.2": ">=7.5.2",
      "get-func-name@<2.0.1": ">=2.0.1",
      "@babel/traverse@<7.23.2": ">=7.23.2",
      "braces@<3.0.3": ">=3.0.3"
    }
  }
}
