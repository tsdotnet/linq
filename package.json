{
  "name": "@tsdotnet/linq",
  "version": "1.0.11",
  "description": "A familiar set of functions that operate on JavaScript iterables (ES2015+) in a similar way to .NET's LINQ does with enumerables.",
  "author": "electricessence",
  "license": "MIT",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "module": "dist-esm/index.js",
  "keywords": [
    "linq",
    "iterator",
    "iterators",
    "iterable",
    "iterables",
    "enumerable",
    "es2015",
    "filter",
    "map",
    "select",
    "reduce",
    "aggregate",
    "where",
    "order by",
    "order-by",
    "orderBy",
    "group by",
    "group-by",
    "groupBy",
    "filters",
    "transforms",
    "resolutions",
    "resolve"
  ],
  "scripts": {
    "build:dist": "rimraf dist/* && copyfiles -u 1 src/**/*.d.ts dist && tsc",
    "build:dist-esm": "rimraf dist-esm/* && tsc -p tsconfig.esm.json",
    "build": "pnpm i && run-p build:*",
    "bump": "npm run precommit && npm version patch",
    "docs": "rimraf docs/* && rimraf docs/.nojekyll && typedoc --options typedoc.json --readme none",
    "lint": "eslint src/**/*.ts",
    "precommit": "pnpm i && run-p lint test && run-p build:* && npm run validate && git status -s",
    "prepublishOnly": "npm run build && run-p validate test",
    "preversion": "run-p lint test",
    "postversion": "git push && git push --tags && npm run docs && git commit -m \"Updated docs.\" docs && git push",
    "test": "mocha -r ts-node/register tests/**/*.ts",
    "validate": "node ./.build/validate-package.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsdotnet/linq.git"
  },
  "bugs": {
    "url": "https://github.com/tsdotnet/linq/issues"
  },
  "homepage": "https://github.com/tsdotnet/linq#readme",
  "devDependencies": {
    "@types/chai": "^4.2.13",
    "@types/mocha": "^8.0.3",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "chai": "^4.2.0",
    "copyfiles": "^2.4.0",
    "eslint": "^7.11.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "mocha": "^8.1.3",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "typedoc": "^0.19.2",
    "typedoc-plugin-external-module-name": "3.1.0",
    "typedoc-plugin-nojekyll": "^1.0.1",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@tsdotnet/common-interfaces": "^1.0.4",
    "@tsdotnet/compare": "^1.3.13",
    "@tsdotnet/exceptions": "^1.0.16",
    "@tsdotnet/integer": "^1.0.11",
    "@tsdotnet/linked-node-list": "^1.3.13",
    "@tsdotnet/memoize": "^1.0.9",
    "@tsdotnet/queue": "^1.2.2",
    "tslib": "^2.0.0"
  }
}
